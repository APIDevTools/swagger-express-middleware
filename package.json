  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "samples"
  ],
  "engines": {
    "node": ">=6",
    "npm": ">=4.0"
  },
  "scripts": {
    "lint": "eslint lib test/fixtures test/specs --fix",
